{
  "Application": {},
  "Infrastructure": {
    "Persistence": {
      "Postgres": {
        "Host": "",
        "Port": -1,
        "Database": "",
        "Username": "",
        "Password": "",
        "SslMode": "Prefer",
        "Pooling": true
      }
    },
    "MessagePersistence": {
      "Persistence": {
        "SchemaName": "message_persistence"
      }
    },
    "External": {
      "AccommodationService": {
        "Address": "http://localhost:1111"
      }
    }
  },
  "Presentation": {
    "Kafka": {
      "Host": "",
      "Consumers": {
        "Bookings": {
          "Topic": "bookings",
          "BufferSize": 100,
          "BufferWaitLimit": "00:00:01",
          "ParallelismDegree": 1,
          "Group": "notification-enrichment-service[bookings]",
          "Inbox": {
            "BatchSize": 100,
            "PollingDelay": "00:00:01",
            "RetryCount": 10
          }
        }
      },
      "Producers": {
        "BookingNotifications": {
          "Topic": "booking_notifications",
          "Outbox": {
            "BatchSize": 100,
            "PollingDelay": "00:00:01",
            "RetryCount": 10
          }
        }
      }
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "gRPC": {
        "Url": "http://*:8070",
        "Protocols": "Http2"
      }
    }
  },
  "Platform": {
    "ServiceName": "<>",
    "Observability": {
      "Tracing": {
        "IsEnabled": false
      },
      "Metrics": {
        "IsEnabled": false
      },
      "Sentry": {
        "IsEnabled": false,
        "Configuration": {
          "Dsn": "",
          "Debug": false,
          "TracesSampleRate": 1.0,
          "MinimumEventLevel": "Warning"
        }
      },
      "Logging": {
        "Serilog": {
          "MinimumLevel": {
            "Default": "Verbose",
            "Override": {
              "Microsoft.Hosting.Lifetime": "Information",
              "Microsoft.AspNetCore": "Warning",
              "Microsoft.Extensions.Http": "Information",
              "System.Net.Http.HttpClient": "Information",
              "Grpc.Net.Client": "Information",
              "Grpc.AspNetCore.Server": "Information",
              "Npgsql": "Information"
            }
          }
        }
      }
    }
  }
}
